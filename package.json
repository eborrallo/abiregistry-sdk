{
  "name": "@abiregistry/sdk",
  "version": "0.1.2",
  "description": "Official SDK for ABI Registry - Push and pull smart contract ABIs seamlessly",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "abiregistry": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "abi",
    "registry",
    "smart-contracts",
    "ethereum",
    "web3",
    "blockchain",
    "solidity",
    "typescript",
    "viem",
    "abi-registry",
    "abiregistry",
    "abiregistry-sdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eborrallo/abiregistry-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/eborrallo/abiregistry-sdk/issues"
  },
  "homepage": "https://github.com/eborrallo/abiregistry-sdk#readme",
  "author": "ABI Registry",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  }
}